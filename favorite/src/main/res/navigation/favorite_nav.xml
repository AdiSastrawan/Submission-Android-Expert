<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@id/favoriteFragment2">
    <fragment
        android:id="@+id/favoriteFragment2"
        android:name="com.adisastrawan.favorite.home.FavoriteFragment"
        android:label="FavoriteFragment" >
        <action
            android:id="@+id/action_favoriteFragment2_to_detailFragment2"
            app:destination="@id/detailFragment2" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment2"
        android:name="com.adisastrawan.androidexpertproject.detail.DetailFragment"
        android:label="DetailFragment" >
        <argument android:name="news" app:argType="com.adisastrawan.core.domain.model.News"  />
        <action
            android:id="@+id/action_detailFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>